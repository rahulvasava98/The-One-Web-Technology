@{
    ViewData["Title"] = "trainingInqurieForm";
    Layout = "~/Views/Shared/_Global_Layout.cshtml";
}
@section header {
    <title>Training Inquiry Form | The One Web Technology  </title>
}

@section meta {

    <meta property="og:image:height" content="150">
    <meta property="og:image:width" content="150">
    <meta property="og:content" content="Training Inquiry Form" />
    <meta property="og:title" content="Training Inquiry Form  | The One Web Technology " />
    <meta property="og:description" content="Our Offices Around The World" />
    <meta property="og:image" itemprop="image" content="https://dummyimage.com/600x400/000/fff" />

}


<section class="contact-us-form-section  mega-section my-5">
    <div class="container my-5">
        <div class="row">
            <div class="col-12 ">
                <div class="contact-form-panel">
                    <div class="sec-heading centered    ">
                        <div class="content-area">
                            <h2 class=" title    wow fadeInUp" data-wow-delay=".4s">Traning Enquiry Form</h2>
                        </div>
                    </div>
                    <div class="contact-form-inputs wow fadeInUp" data-wow-delay=".6s">
                        <div class="custom-form-area input-boxed">
                            <!--Form To have user messages-->
                            <form class="main-form" action="https://amincode.com/html/flex-it/php/send-mail.php">
                                <span class="done-msg"></span>
                                <div class="row ">
                                    @* name start *@
                                    <div class="col-12 col-lg-6">
                                        <div class="   input-wrapper">
                                            <input class="text-input" type="text" />
                                            <label class="input-label"> First name <span class="req">*</span></label>
                                            <span class="b-border"></span><span class="error-msg"></span>
                                        </div>
                                    </div>

                                    <div class="col-12 col-lg-6">
                                        <div class="   input-wrapper">
                                            <input class="text-input" type="text" />
                                            <label class="input-label">Last name <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                        </div>
                                    </div>
                                    @* name end *@

                                    @* contact start  *@
                                    <div class="col-12 col-lg-6">
                                        <div class="   input-wrapper">
                                            <input class="text-input" type="email" />
                                            <label class="input-label"> Phone Number <span class="req">*</span></label>
                                            <span class="b-border"></span><span class="error-msg"></span>
                                        </div>
                                    </div>
                                    <div class="col-12 col-lg-6">
                                        <div class="   input-wrapper">
                                            <input class="text-input" type="email" />
                                            <label class="input-label"> E-mail <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                        </div>
                                    </div>
                                    @* contact  end  *@


                                    <div class="col-12 ">
                                        <div class="   input-wrapper">
                                            <input class="text-input" type="text" />
                                            <label class="input-label"> Education qualification <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                        </div>
                                    </div>
                                    <div class="col-12 ">
                                        <div class="   input-wrapper">
                                            <input class="text-input" type="text" />
                                            <label class="input-label"> Location <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                        </div>
                                    </div>
                                    <div class="col-12 ">
                                        <div class="   input-wrapper">
                                            <textarea class=" text-input" placeholder="In which field you want to pursue you internship"></textarea>
                                            <label class="input-label"> your message <span class="req">*</span></label><span class="b-border"></span><i></i><span class="error-msg"></span>
                                        </div>
                                    </div>
                                    @* upload resume start  *@

                                    <div class="col-12">
                                        <div class="input-wrapper">
                                            <input type="file" id="myFile">
                                            <label class="input-label" for="msg-subject"> Upload resume <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>

                                        </div>

                                    </div>


                                    @* upload resume end  *@
                                    <div class="col-12 submit-wrapper">
                                        <button class=" btn-solid" type="submit">Send your message</button>
                                    </div>




                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>














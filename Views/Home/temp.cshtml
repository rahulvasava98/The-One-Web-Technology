    <script>
        $(document).ready(function () {
            $(".slider_images_1").slick({
                slidesToShow: 3.6,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 1000,
                arrows: false,
                dots: false,
                pauseOnHover: false,
                navigation: {
                    nextEl: ".next_btn",
                    prevEl: ".precvious_btn"
                },
                responsive: [
                    {
                        breakpoint: 992,
                        settings: {
                            slidesToShow: 4
                        }
                    },

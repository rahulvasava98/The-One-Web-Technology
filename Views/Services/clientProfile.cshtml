                                                    <div class="custom-form-area input-boxed">
                                                        <!--Form To have user messages-->
                                                        <div class="row">
                                                            <div class="col-10 m-auto">


                                                                <form id="clientUseDataEditForm" class="main-form" asp-action="clientProfile">
                                                                    <span class="done-msg"></span>
                                                                    <div class="row ">
                                                                        <div class="col-12">
                                                                            <div class="input-wrapper">
                                                                                <input type="hidden" asp-for="clientId" />
                                                                                <input type="hidden" asp-for="clientReferenceId" />
                                                                                <input type="hidden" asp-for="clientReferenceCode" />
                                                                                <input class="text-input" type="text" asp-for="clientfirstName" />
                                                                                <label class="input-label"> First Name <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-12">
                                                                            <div class="   input-wrapper">
                                                                                <input class="text-input" type="text" asp-for="clientlastName" />
                                                                                <label class="input-label"> Last Name <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>
                                                                        
                                                                        <div class="col-12">
                                                                            <div class="   input-wrapper">
                                                                                <input class="text-input" type="text" asp-for="clientemail" readonly />
                                                                                <label class="input-label"> Email <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>


                                                                        <div class="col-12">

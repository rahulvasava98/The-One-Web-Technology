                                                                                <input class="text-input" type="text" asp-for="clientservice" />
                                                                                <label class="input-label"> Client Service <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-12">
                                                                            <div class="   input-wrapper">
                                                                                <input class="text-input" type="text" asp-for="clientlocation" />
                                                                                <label class="input-label"> Location <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>                                                                      
                                                                        <div class="col-12">
                                                                            <div class="   input-wrapper">
                                                                                <input class="text-input" type="text" asp-for="clientmessage" />
                                                                                <label class="input-label"> client Message <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-12">
                                                                            <div class="   input-wrapper">
                                                                                <input class="text-input" type="text" asp-for="clientPassword" readonly />
                                                                                <label class="input-label"> Password <span class="req">*</span></label><span class="b-border"></span><span class="error-msg"></span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-12 submit-wrapper">
                                                                            <button class="btn-solid" type="submit">Submit</button>
                                                                        </div>
                                                                    </div>
                                                                </form>


                                                                <table style="display:none;border:1px solid;border-right:none;" id="usereferrelDataTable" class="table">
                                                                    <thead>
                                                                        <tr>
                                                                            <th style="border:1px solid;">
                                                                                <ul>
                                                                                    <li>Id</li>
                                                                                </ul>
                                                                            </th>
                                                                            <th style="border:1px solid;">
                                                                                <ul>
                                                                                    <li>Data</li>
                                                                                </ul>

                                                                            </th>
                                                                            <th style="border:1px solid;">
                                                                                <ul>
                                                                                    <li>
                                                                                        referral Data
                                                                                    </li>
                                                                                </ul>


                                                                            </th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        @foreach (var item in Model.serviceRegistrationModelLists)
                                                                        {
                                                                            <tr>
                                                                                <td style="border:1px solid;">
                                                                                    @item.clientId
                                                                                </td>
                                                                                <td style="border:1px solid;">
                                                                                    <ul>
                                                                                        <li>firstName : @item.clientfirstName</li>
                                                                                        <li>lastName : @item.clientlastName</li>
                                                                                        <li>Email : @item.clientemail </li>
                                                                                        <li>Location : @item.clientlocation</li>
                                                                                        <li>Service : @item.clientservice </li>
                                                                                        <li>Your message : @item.clientmessage</li>
                                                                                        <li>ServiceUserPassword : @item.clientPassword </li>

                                                                                    </ul>
                                                                                </td>
                                                                                <td style="border:1px solid;">
                                                                                    <ul>
                                                                                        <li>referenceId : @item.clientReferenceId </li>
                                                                                        <li>referenceCode :@item.clientReferenceCode</li>
                                                                                    </ul>
                                                                                </td>
                                                                            </tr>
                                                                        }
                                                                    </tbody>
                                                                </table>

                                                                @* list end *@
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>


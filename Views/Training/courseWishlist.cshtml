@{
    ViewData["Title"] = "courseWishlist";
}
<style>    
    .courseWishlist_banner_section {
        margin-top: 96px;
        background-color: var(--clr-main-variation);
    }

    .courseWishlist_tab_list {
        list-style: none;
    }

    .tab_1_schedule_learning_card {
        border: 1px solid var(--clr-semi-dark);
    }

    .tab_1_schedule_learning_wrapper {
        padding: 20px;
    }

    .coursewishlist_secondary_para {
        width: 80%;
    }

    .tab_1_card_para {
        color: var(--clr-semi-dark);
        font-size: 0.9rem;
    }

    .tab_1_card_content_container {
        padding: 10px;
    }

    .tab_1_card_course_rating i {
        font-size: 0.9rem;
    }

    .tab_1_all_courses_card_container {
        margin-top: 50px;
    }

    .tab_1_courses_card_container {
        border: 1px solid var(--clr-main);
        overflow: hidden;
    }

    .tab_1_all_courses_card {
        margin-top: 50px;
    }

    .btn-model-heading-wrapper {
        text-align: center;
    }

    .btn-model-close-btn-wrapper {
        text-align: center;
    }

    .modal-content {
        background-color: transparent !important;
    }

    .btn-close i {
        color: var(--clr-white);
    }

    .leave-rating-model-star {
        text-align: center;
    }

        .leave-rating-model-star i {
            font-size: 2rem;
            padding: 1rem;
            /* font-weight:900; */
        }

    .leave-rating-model-star-container {
        padding: 4rem;
    }

    .third_tab_search_btn {
        /* background-color:transparent; */
        border: none;
        background-color: #09aff4;
        padding: 6px 10px;
    }


    .coursewishlist_tab_third_search {
        background-color: transparent;
        border: 1px solid var(--clr-main);
        padding: 5px 10px;
        /* border-radius:8px; */
        color: var(--clr-white);
    }

        .coursewishlist_tab_third_search:focus-visible {
            outline: none;
        }

    .wishlist_thumbnail_images_container {
        max-width: 304px;
        max-height: 170px;
        position: relative;
        z-index: 1;
    }

    .wishlist_thumbnail_icon {
        position: absolute;
        font-size: 1.3rem;
        top: 5px;
        right: 10px;
    }

    .courseWishList_mylist_tab {
        text-align: center;
        padding: 100px 0;
    }

    .courseWishlist_tab_link {
        background-color: inherit;
        border: none;
        outline: none;
        cursor: pointer;
        padding: 14px 16px;
        transition: 0.5s;
        color: var(--clr-white);
    }

        .courseWishlist_tab_link.active {
            background-color: var(--clr-main);
        }

    .coursewishlite_tab_content {
        display: block;
        transition: 0.5s;
    }

</style>

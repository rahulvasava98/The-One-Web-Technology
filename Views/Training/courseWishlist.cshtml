<script>
    function courseWishlistLike() {
        let iconElement = document.getElementById("wishlist_card_icon");
        ;
        if (iconElement.style.fontWeight != "900") {
            iconElement.style.fontWeight = "900";
        }
        else if (iconElement.style.fontWeight != "0") {
            iconElement.style.fontWeight != "0";
        }
      
        // console.log("djos jod josdjo jodjo osjo pop kp");
    }
</script>

  

<script>
    function wihslistTab(evt,navList){
        

        // document.getElementById("coursewishlist_tab_all_courses").style.display = "block";
        // document.getElementById("coursewishlist_tab_my_lists").style.display = "block";
        // document.getElementById("coursewishlist_tab_wishlist").style.display = "block";


        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("coursewishlite_tab_content");
        console.log(tabcontent.item);

        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }

        tablinks = document.getElementsByClassName("courseWishlist_tab_link");
        console.log(tablinks.item);

        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace("active", "");
        }
        document.getElementById(navList).style.display = "block";
        evt.currentTarget.className += " active";

    }
</script>
